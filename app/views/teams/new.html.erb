<%= form_for [current_user, @team] do |f| %>
 <p>
  <%= f.label :title %><br>
  <%= f.text_field :title %>
 </p>
 <p>
  <%#= f.label :member %><br>
  <%#= f.text_area :member %>
 </p>
  <p>
  	<%#= f.label :role %><br>
  	<%#= select_tag :role, Membership.roles.keys %>
  	<%#= f.select :role, ['Admin', 'Project Manager' , 'Team Leader' , 'Employee']%>
	 </p>
 <p>
  <%= f.submit %>
 </p>
<% end %>