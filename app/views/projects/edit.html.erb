<h1>Edit Projects</h1>
<%= form_for [@project, :team_id] do |f| %>
	<p>
	 	<%= f.label :name %><br>
	 	<%= f.text_field :name %>
	</p>
	<p>
	 	<%= f.label :start_date %><br>
	 	<%= f.text_field :start_date %>
	</p>
	<p>
	 	<%= f.label :end_date %><br>
	  <%= f.text_field :end_date %>
	</p>
  	<%= f.submit %>
  </p>
<% end %>