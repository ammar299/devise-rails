	<%= form_for [current_user, @team, @project] do |f| %>
	<p>
	 	<%= f.label :name %><br>
	 	<%= f.text_field :name %>
	<p>
	<p>
	 	<%= f.label :start_date %><br>
	 	<%= f.date_field :start_date %>
	</p>
	<p>
	 	<%= f.label :end_date %><br>
	  <%= f.date_field :end_date %>
	</p>
		<%#= f.hidden_field :project_id, value: @project.id%>
		<%= f.hidden_field :team_id, value: @team.id%>
  	<%= f.submit %>
  </p>
<% end %>