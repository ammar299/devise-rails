
 <header class=".bg-blue-400 text-white">
    <div class="container text-left">
      <h1>CREATE PROJECT</h1>
      <%= form_for [current_user, @team, @project] do |f| %>
          <h1><%= f.label :name %></h1>
          <h2><%=f.text_field :name %></h2>

          <h1><%= f.label :start_date %></h1>
          <h2><%=f.date_field :start_date %></h2>

          <h1><%= f.label :end_date %></h1>
          <h2><%=f.date_field :end_date %></h2>

          <%= f.hidden_field :team_id, value: @team.id%>

          <div class="form-group">
            <%=f.submit class: " btn btn-primary btn-lg"%>
          </div>
            <% end %>
    </div>
  </header>